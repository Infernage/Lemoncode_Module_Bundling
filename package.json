{
  "name": "vue-webpack-esbuild",
  "version": "1.0.0",
  "scripts": {
    "start": "webpack serve --config config/webpack.dev.js --progress",
    "build": "run-p -l type-check build:prod",
    "build:perf": "webpack --config config/webpack.perf.js --progress",
    "build:prod": "webpack --config config/webpack.prod.js --progress",
    "build:dev": "webpack --config config/webpack.dev.js --progress",
    "type-check": "tsc -p ./tsconfig.json --noEmit"
  },
  "dependencies": {
    "core-js": "^3.19.1",
    "vue": "^3.2.21"
  },
  "devDependencies": {
    "@vue/compiler-sfc": "^3.2.21",
    "@vue/eslint-config-typescript": "^9.0.1",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.1.3",
    "dotenv-webpack": "^7.0.3",
    "esbuild-loader": "^2.16.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.4.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "typescript": "^4.4.4",
    "vue-loader": "16.5.0",
    "vue-style-loader": "^4.1.3",
    "webpack": "^5.64.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0",
    "webpack-merge": "^5.8.0"
  }
}
